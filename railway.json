{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "startCommand": "pip install -U -r requirements.txt && python3 -m JoKeRUB"
  },
  "variables": {
    "API_ID": "your_api_id_value_here",
    "API_HASH": "your_api_hash_value_here"
  }
}
